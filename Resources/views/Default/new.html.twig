{% extends '@TraitUpload/Default/template.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}
    {% block Gallery %}

        <div class="row-fluid">
            {{ form_start(form) }}
            {{ form_errors(form) }}

            <div class="form-group">
                {{ form_label(form.name) }}
                <div class="input-group">

                    {{ form_widget(form.name) }}
                </div>
            </div>

            <div class="form-group">
                {{ form_label(form.file) }}
                <div class="input-group">

                    {{ form_widget(form.file) }}
                </div>
            </div>

            {{ form_rest(form) }}
            <button type="submit" class="btn btn-default">Enregistrer</button>

            {{ form_end(form) }}
        </div>

    {% endblock %}
{% block javascripts %}
    {{ parent() }}
{% endblock %}
